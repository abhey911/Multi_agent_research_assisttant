# Google Gemini API Key
GOOGLE_API_KEY=your_google_api_key_here

# Free API Keys (Get from respective providers)
# Groq: https://console.groq.com/keys (14,400 requests/day FREE)
GROQ_API_KEY=

# OpenRouter: https://openrouter.ai/keys (Free tier available)
OPENROUTER_API_KEY=

# Together AI: https://api.together.xyz/settings/api-keys ($25 free credit)
TOGETHER_API_KEY=

# Hugging Face API Key (optional - for Hugging Face inference)
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# SerpAPI Key (optional - for Google search)
SERPAPI_API_KEY=your_serpapi_key_here


# Model Configuration
GEMINI_MODEL_FLASH=gemini-2.0-flash
GEMINI_MODEL_PRO=gemini-2.5-pro
GROQ_MODEL=llama-3.3-70b-versatile
OPENROUTER_MODEL=meta-llama/llama-3.1-8b-instruct:free
TOGETHER_MODEL=meta-llama/Meta-Llama-3.1-8B-Instruct-Turbo

# Provider Priority (groq, openrouter, together, google)
PRIMARY_PROVIDER=groq

# Hugging Face Model Configuration (alternative to Gemini)
HF_MODEL=meta-llama/Llama-2-70b-chat-hf
USE_HUGGINGFACE=false

# Research Configuration
MAX_SEARCH_RESULTS=3
MAX_ITERATIONS=2
RESEARCH_DEPTH=standard
REQUEST_DELAY=3.0
MAX_RETRIES=2
